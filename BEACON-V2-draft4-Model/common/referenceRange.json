{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReferenceRange",
  "description": "Definition of a range class.",
  "type": "object",
  "properties": {
    "units": {
      "description": "The kind of unit. RECOMMENDED.",
      "$ref": "https://raw.githubusercontent.com/MrRobb/beacon-v2-Models/main/BEACON-V2-draft4-Model/common/commonDefinitions.json#/definitions/Units",
      "examples": [{ "id": "NCIT:C49670", "label": "Millimeter of Mercury" }]
    },
    "low": {
      "description": "Lower range end of normal",
      "type": "number",
      "examples": [85]
    },
    "high": {
      "description": "Upper range end of normal",
      "type": "number",
      "examples": [135]
    }
  },
  "required": ["low", "high"],
  "additionalProperties": true
}
