{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReferenceRange",
  "description": "Definition of a range class.",
  "type": "object",
  "properties": {
    "unit" : {
      "description": "The kind of unit. REQUIRED.",
      "$ref": "https://raw.githubusercontent.com/ga4gh-beacon/beacon-framework-v2/main/common/ontologyTerm.json",
      "example": {
        "id" : "NCIT:C28253",
        "label" : "Milligram"
      }
    },
    "low" : {
      "description" : "Lower range end of normal. REQUIRED.",
      "type" : "float"
    },
    "high" : {
      "description" : "Upper range end of normal. REQUIRED.",
      "type" : "float"
    }
  },
  "required": ["low", "high"],
  "additionalProperties": true
}
